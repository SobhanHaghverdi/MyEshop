@model IEnumerable<Product>
@{
    ViewData["Title"] = ViewData["GroupName"];
}
<div class="pl-5">
    <h1 class="text-center mt-4">دوره های @ViewData["GroupName"]</h1>
    <hr class="mt-4 w-75" />
    <div class="row">
        @foreach (var item in Model)
        {
            <partial name="_BoxProduct" model="@item" />
        }
    </div>
</div>

